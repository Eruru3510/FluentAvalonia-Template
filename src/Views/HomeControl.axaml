<UserControl
	x:Class="AvaloniaNav.HomeControl"
	xmlns="https://github.com/avaloniaui"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	xmlns:vm="using:AvaloniaNav.ViewModels"
	d:DesignHeight="650"
	d:DesignWidth="1500"
	x:CompileBindings="True"
	x:DataType="vm:HomeControlViewModel"
	mc:Ignorable="d">

	<Design.DataContext>
		<vm:HomeControlViewModel />
	</Design.DataContext>

	<Border Margin="10" Background="White">
		<TextBlock Padding="10" Foreground="Black">Avalonia Nav</TextBlock>
	</Border>
</UserControl>
